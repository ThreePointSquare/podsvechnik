{
  "name": "English syntax highlighter",
  "version": "1.0",
  "description": "Podsvechnik - English syntax highlighter",
  "permissions": ["activeTab", "contextMenus"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
